{
  "name": "bachelors-subscription",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.0"
  },
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon ./src/bin/www"
  },
  "_moduleAliases": {
    "root": "./src",
    "config": "./src/config",
    "services": "./src/services"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "jest",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^root(.*)$": "<rootDir>/src/$1",
      "^config(.*)$": "<rootDir>/src/config/$1",
      "^services(.*)$": "<rootDir>/src/services/$1"
    },
    "projects": [
      {
        "displayName": "test"
      },
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/src/**/*.{js,jsx}"
        ]
      }
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "blueimp-md5": "^2.12.0",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.0.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.1.0",
    "moleculer": "^0.13.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "jest-mock-express": "^0.1.1",
    "jest-runner-eslint": "^0.6.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "supertest": "^3.1.0"
  }
}
